    <key width="1.5" role="action" key0="shift" key2="loc capslock"/>
    <key key0="z"/>
    <key key0="x" key2="loc †"/>
    <key key0="m"/>
    <key key0="c" key2="&lt;" key3="."/>
    <key key0="v" key2="&gt;" key3=","/>
    <key key0="k" key2="\?" key3="/"/>
    <key key0="l" key2="&quot;" key3="'"/>
