<?xml version="1.0" encoding="utf-8"?>
<!-- The bottom row used in the emoji pane. -->
<keyboard bottom_row="false">
  <row height="0.95">
    <key key0="switch_back_emoji"/>
    <key width="4" key0="space" key5="cursor_left" key6="cursor_right" slider="true"/>
    <key key0="backspace"/>
    <key key0="enter"/>
  </row>
</keyboard>
